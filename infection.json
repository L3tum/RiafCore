{
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php://stdout"
    },
    "mutators": {
        "@default": true,
        "@arithmetic": true,
        "@boolean": true,
        "@cast": true,
        "@conditional_boundary": true,
        "@conditional_negotiation": true,
        "@equal": true,
        "@function_signature": true,
        "@identical": true,
        "@number": true,
        "@operator": true,
        "@regex": true,
        "@removal": true,
        "@return_value": true,
        "@sort": true,
        "@unwrap": true,
        "@extensions": true,
        "NotIdenticalNotEqual": {
            "ignore": [
                "Riaf\\Core"
            ]
        }
    }
}
